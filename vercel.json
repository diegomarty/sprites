{
    "version": 2,
    "builds": [
        {
            "src": "api/app.js",
            "use": "@now/node-server"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/sprites/(.*)",
            "dest": "sprites/$1"
        }
    ]
}


