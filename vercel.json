{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "source": "/(.*)",
            "destination": "app.js"
        },
        {
            "source": "/sprites/(.*)",
            "destination": "sprites/$1"
        }
    ]
}


